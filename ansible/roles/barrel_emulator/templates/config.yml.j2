# SwitchBot K10 dock/barrel emulator config (generated by Ansible)
k10_emulator:
  adapter: {{ k10_emulator.adapter | default("hci0") }}
  local_name: {{ k10_emulator.local_name | default("WoS1MB") }}

  fw_major: {{ k10_emulator.fw_major | default(1) }}
  fw_minor: {{ k10_emulator.fw_minor | default(0) }}

  company_id: {{ k10_emulator.company_id | default("0x0969") }}
  manufacturer_mac_label: {{ k10_emulator.manufacturer_mac_label | default(none) }}

  include_tx_power: {{ (k10_emulator.include_tx_power | default(true)) | bool }}
  advertise_fd3d_service_data: {{ (k10_emulator.advertise_fd3d_service_data | default(true)) | bool }}
  fd3d_service_data_hex: "{{ k10_emulator.fd3d_service_data_hex | default("00") }}"

  advertise_service_uuids:
{% for uuid in (k10_emulator.advertise_service_uuids | default([])) %}
    - "{{ uuid }}"
{% endfor %}

